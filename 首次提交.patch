+
+                switch (TYPE) {
+                    case 0://真实姓名
+                        userInfo.setUserName(content);
+                        break;
+                    case 1://身份证号
+                        if (!IdcardUtils.validateCard(content)) {
+                            MyToastUtils.showShortToast(getApplicationContext(), "请输入合法的身份证号");
