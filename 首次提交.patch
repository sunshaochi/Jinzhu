+                    cityinfo.setId(array.get(i).getAsJsonArray().get(1)
+                            .getAsString());
+                    city_list_code.add(array.get(i).getAsJsonArray().get(1)
+                            .getAsString());
+                    list.add(cityinfo);
+                }
+                hashMap.put(entry.getKey(), list);
+            }
+            return hashMap;
+        }
