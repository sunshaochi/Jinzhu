+            if(!TextUtils.isEmpty(jsonType)) {
+                MessageBean mb = GsonUtils.json2Bean(jsonType, MessageBean.class);
+                mb.setTime(FinancialUtil.getCurrentTime());
+                mb.setMsg_id(bundle.getString(JPushInterface.EXTRA_MSG_ID));
