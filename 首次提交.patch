+			if (dh <= getHeight() - mVerticalPadding * 2&& dw >= getWidth() - mHorizontalPadding * 2)
+			{//如果图片的高度<截图区高度，并且图片宽度>截图区宽度时
+				scale = (getHeight() * 1.0f - mVerticalPadding * 2) / dh;
+			}
